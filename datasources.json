{
  "localDb": {
    "name": "localDb",
    "connector": "postgresql",
    "url": "",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "user": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME}",
    "autoupdate": true
  },

  "remoteDb": {
    "name": "remoteDb",
